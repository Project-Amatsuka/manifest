<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <!-- AOSP -->
  <remote name="aosp"
          fetch="https://android.googlesource.com"
          sync-c="true"
          sync-tags="false"
          review="https://android-review.googlesource.com/"
          revision="refs/tags/android-13.0.0_r16"/>

  <!-- Github -->
  <remote  name="github"
           fetch="https://github.com" />

  <!-- Gitlab -->
  <remote  name="gitlab"
           fetch="https://gitlab.com" />

  <!-- CodeLinaro -->
  <remote  name="la_system"
           fetch="https://git.codelinaro.org/clo/la" />

  <remote  name="la_vendor"
           fetch="https://git.codelinaro.org/clo/la"
           revision="refs/tags/LA.UM.9.14.r1-21000-LAHAINA.QSSI13.0" />

  <default revision="refs/tags/LA.QSSI.13.0.r1-08600.01-qssi.0"
           remote="la_system"/>

  <!-- CodeLinaro XML -->
  <include name="snippets/system.xml" />
  <include name="snippets/vendor.xml" />

  <!-- Nyanpasu XML -->
  <include name="snippets/obsolete.xml" />
  <include name="snippets/nyanpasu.xml" />
</manifest>
