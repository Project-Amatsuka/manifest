<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <!-- AOSP -->
  <remote name="aosp"
          fetch="https://android.googlesource.com"
          sync-c="true"
          sync-tags="false"
          review="https://android-review.googlesource.com/"
          revision="refs/tags/android-13.0.0_r24"/>

  <!-- Github -->
  <remote  name="github"
           fetch="https://github.com" />

  <!-- Gitlab -->
  <remote  name="gitlab"
           fetch="https://gitlab.com" />

  <!-- CodeLinaro -->
  <remote name="clo"
          fetch="https://git.codelinaro.org/clo/la"
          review="codelinaro.org" />

  <default remote="clo"
           revision="partner-android/r-fs-release" />

  <!-- CodeLinaro XML -->
  <include name="snippets/system.xml" />
  <include name="snippets/vendor.xml" />

  <!-- Nyanpasu XML -->
  <include name="snippets/obsolete.xml" />
  <include name="snippets/nyanpasu.xml" />
</manifest>
